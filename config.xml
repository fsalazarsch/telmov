<?xml version='1.0' encoding='utf-8'?>
<widget id="cl.teletonmovil.cityex" version="0.0.1" versionCode="100" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Bitacora teleton</name>
    <description>
        Bitacora Elctronica para la telet√≥n
    </description>
    <author email="fsalazar.sch@gmail.com" href="http://www.nullpointerex.com">
        Felipe Salazar Schlotterbeck
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-geolocation" />
    <icon src="www/res/icon/icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="ios" spec="~4.1.0" />
    <engine name="android" spec="~5.1.1" />
    <engine name="windows" spec="~4.3.1" />
    <plugin name="cordova-plugin-background-mode" spec="~0.7.2" />
</widget>
